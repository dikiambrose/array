#include <iostream>

using namespace std;


int main(int argc, char** argv) {
 
 int pilih;
 int arrayyy [3] = {3,4,5};
 
 cout << "Index ke : ";
 
 cin >> pilih;
 
 cout << "nilai dari index ke-" << pilih <<" adalah : " << arrayyy[pilih-1]<<"\n\n";
 
 
 for(int a=0; a < 3; a++)
 {
  
  cout << "index ke " << a << ": ";
  cout << " "<<arrayyy[a] << "\n";
  
  
 }
 
 
 
 cout << "\n\t\tdikiambrose.com";
 
 
 
 return 0;
}
